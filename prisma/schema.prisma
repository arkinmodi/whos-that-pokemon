// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider      = "prisma-client-js"
    binaryTargets = ["native"]
}

datasource db {
    // provider = "sqlite"
    // url      = "file:./db.sqlite"

    provider = "postgres"
    url      = env("DATABASE_URL")
}

model Pokemon {
    id        Int    @id
    name      String
    spriteUrl String
}
